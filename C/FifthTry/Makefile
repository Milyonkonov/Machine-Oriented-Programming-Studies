.PHONY: clean All

All:
	@echo "----------Building project:[ flipflip_irq - Debug ]----------"
	@cd "flipflip_irq" && "$(MAKE)" -f  "flipflip_irq.mk" && "$(MAKE)" -f  "flipflip_irq.mk" PostBuild
clean:
	@echo "----------Cleaning project:[ flipflip_irq - Debug ]----------"
	@cd "flipflip_irq" && "$(MAKE)" -f  "flipflip_irq.mk" clean
